{
	"name" : "beotie/psr7_request_dto_bridge",
	"description" : "This package provide a bridge system to convert PSR7 request to DTO",
	"type" : "library",
	"license" : "MIT",
	"authors" : [{
			"name" : "matthieu vallance",
			"email" : "matthieu.vallance@cscfa.fr"
		}
	],
	"minimum-stability" : "dev",
	"require" : {
		"beotie/psr7_request": "^1",
		"beotie/dto": "^1",
		"symfony/options-resolver": "^3.0@stable || ^4.0@stable"
	},
	"require-dev" : {
		"phpunit/phpunit" : "^6.3@stable",
		"phpmd/phpmd" : "^2.6@stable",
		"squizlabs/php_codesniffer" : "^3.0@stable",
		"sebastian/phpcpd" : "^3.0@stable",
		"phpmetrics/phpmetrics" : "^2.2@stable"
	},
	"autoload" : {
		"psr-4" : {
			"Beotie\\PSR7\\DTOBridge\\" : "Beotie/PSR7/DTOBridge"
